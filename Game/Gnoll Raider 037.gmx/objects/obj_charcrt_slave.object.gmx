  }
  crnt_pnum = cjp
  pslot[crnt_pnum] = (cjp + 1)
  //respond immediately
  clearbuffer();
  writebyte(SLAVE_ID);
  writebyte(crnt_pnum);
  writestring(pname[crnt_pnum]);
  //writestring(keyboard_string);
  sendmessage(server);
  cjp += 1;
  break;
    // A connection has been closed with a client
case MASTER_DROP:
    // send player back to join_lobby
    show_message_menu("Host has disconnected");
    return_from = 1;
    break;
    // A new client position has been updated
case MLOBBY_ID:
  cjp = readint()
  for(i=0;i&lt;cjp;i+=1){
    pnum[i] = readint();
    pslot[i] = readint();
    ready[i] = readbyte();
