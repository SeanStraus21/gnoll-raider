            <string>///Listen for Pings
var packet, size, crnt_ip, crnt_name,disconnected;
while(true){
  size = receivemessage(temp_socket);
  if(size &gt; 0){
    crnt_ip = readstring();
    crnt_name = readstring();
    disconnected = readbyte();
    if(!ds_map_exists(session_list,crnt_ip)){
      ds_map_add(session_list,crnt_ip,crnt_name);
    }else{
      if(disconnected){
        ds_map_delete(session_list,crnt_ip);
      }
    }
  }else{
    break;
  }
}
//update the slots
event_user(0);
alarm[0] = 30;

