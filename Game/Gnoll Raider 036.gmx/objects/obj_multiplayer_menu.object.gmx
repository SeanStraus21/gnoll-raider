            <string>///join, create, or quit
//this needs to connect the player to his selected host
switch(hovered.btn_index){
case 0: //Create
  sound_play(snd_click);
  global.multiplayer = true;
  global.var_numberofplayers = 8;
  global.master = true;
  crnt_pnum = 0;
  closesocket(temp_socket);
  ds_map_destroy(session_list);
  room_goto(room_lobby);
  break;
case 1://Join
  sound_play(snd_click);
  if(selected != noone &amp;&amp; !selected.empty){
    global.master = false;
    server = tcpconnect(selected.ip, PORT_MASTER, 1);
    if (server &lt; 0){
      show_message("Unable to connect to server");
      exit;
    }else{
      global.multiplayer = true
      player[0] = server;
      closesocket(temp_socket);
      ds_map_destroy(session_list);
      room_goto(room_lobby);
    }
  }else{
    show_message("You must select a game to join");
  }
  break;
case 2: //Cancel
  sound_play(snd_click);
  global.multiplayer = false;
  closesocket(temp_socket);
  ds_map_destroy(session_list);
  room_goto(room_mainmenu);
  break;
default: //Select a game
  if(!hovered.empty){
    selected = hovered;
    selection_border.x = selected.x;
    selection_border.y = selected.y;
    selection_border.sprite_index = selected.border_sprite;
  }
}
